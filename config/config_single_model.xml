<world>
    <window width="800" height="600" />
    <camera>
        <position x="0" y="1" z="20" />
        <lookAt x="0" y="0" z="0" />
        <up x="0" y="1" z="0" />
        <projection fov="60" near="1" far="1000" />
    </camera>
    
    <group>
        <transform> 
            <translate x="0" y="0" z="0" />
            <rotate time="5" x="0" y="0" z="1" />
            <scale x="1" y="1" z="1" />
        </transform>

        <models>
            <model file="patch.3d">
                <color>
                    <diffuse R="255" G="200" B="140" />
                    <ambient R="15" G="13" B="10" />
                    <specular R="255" G="255" B="255" />
                    <emissive R="0" G="0" B="0" />
                    <shininess value="128" />
                </color>
            </model>
        </models>
    </group>
</world>